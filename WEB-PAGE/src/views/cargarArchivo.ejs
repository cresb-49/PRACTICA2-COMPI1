<!DOCTYPE html>
<html>
  
<head>
  <meta charset='utf-8'>
  <meta http-equiv='X-UA-Compatible' content='IE=edge'>
  <title>Cargar Archivo Wison</title>
  <meta name='viewport' content='width=device-width, initial-scale=1'>
  <%- include ('plantillas/heads.html')%>
</head>

<body>
  <body style="background-color: darkslategrey;">
    <%- include ('plantillas/nav.html')%>
    <div class="container">
      <div>
      </div>
      <form action="/file" method="POST" enctype="multipart/form-data">
        <div class="mb-3">
          <label class="form-label" style="color: white;font-size: larger;"></label>
          <input type="file" class="form-control" id="fileWison" name="fileWison">
        </div>
        <button type="submit" class="btn">Generar Analizador</button>
      </form>
    </div>
    <br>
    <div class="container">
      <% errores.forEach(function(err){ %>
        <div class="container">
          <div class="alert alert-danger" role="alert">
            <%= err %>
          </div>
        </div>
        <br>
      <% }); %>
    </div>
    <%-include ('plantillas/footer.html')%>  
  </body>
</html>